        <!-- Modal HTML -->
        <!-- Model HTML -->



        <div class="modal-dialog modal-login 4">
            <div class="modal-content">
                <div class="modal-header">
                    <div class="avatar"><i class="material-icons">
                            &#xE7FD;
                            <img src="images/pngwing.com.png" alt="" width="60" height="50">
                        </i></div>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                </div>
                <div class="modal-body">
                    <form action="/glass" onclick="return validate()">
                        <div class="form-group row">
                            <div class="form-group row">
                                <input type="text" class="form-control col-sm-11" placeholder="name" id="name"
                                    onkeyup="name1()">
                                <i style="color: lightgreen;" id="tick" hidden="true"
                                    class="fas fa-check-circle col-sm-1"></i>
                                <i style="color: red;" id="mistake" hidden="true" class="fas fa-times col-sm-1"></i>



                                <span> <label for="error" input type="error" id="error"></label></span>
                            </div>





                            <div class="form-group row">
                                <input type="email" class="form-control col-sm-11" onkeyup="validate1()"
                                    placeholder="email" id="email">
                                <i style="color: lightgreen;" id="tick1" hidden="true"
                                    class="fas fa-check-circle col-sm-1"></i>
                                <i style="color: red;" id="mistake1" hidden="true" class="fas fa-times col-sm-1"></i>
                                <label for="error1" input type="error" id="error1"></label>
                            </div>



                            <div class="form-group row">


                                <input type="text" class="form-control col-sm-11" onkeyup="validate3(this.value)"
                                    placeholder="Mobile number" id="number1">

                                <i style="color: lightgreen;" id="tick2" hidden="true"
                                    class="fas fa-check-circle col-sm-1"></i>
                                <i style="color: red;" id="mistake2" hidden="true" class="fas fa-times col-sm-1"></i>
                                <label for="error2" input type="error2" id="error2"></label>
                            </div>






                            <div class="form-group row">

                                <input type="password" placeholder="Password" class="form-control col-sm-11" id="psw"
                                    name="psw" onkeyup="sandeep(this.value)"
                                    title="Must contain at least one number and one uppercase and lowercase letter, and at least 8 or more characters">
                                <i style="color: lightgreen;" id="tick2" hidden="true"
                                    class="fas fa-check-circle col-sm-1"></i>
                                <i style="color: red;" id="mistake2" hidden="true" class="fas fa-times col-sm-1"></i>
                                <label>Password Strength: </label>
                                <div class="form-group">
                                    <div class="progress">
                                        <div id="weak" hidden="true"
                                            class="progress-bar bg-danger progress-bar-striped progress-bar-animated"
                                            role="progressbar" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"
                                            style="width: 25%"></div>
                                        <div id="medium" hidden="true"
                                            class="progress-bar bg-warning progress-bar-striped progress-bar-animated"
                                            role="progressbar" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100"
                                            style="width: 50%"></div>
                                        <div id="strong" hidden="true"
                                            class="progress-bar bg-success progress-bar-striped progress-bar-animated"
                                            role="progressbar" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"
                                            style="width: 100%"></div>
                                    </div>





                                </div>

                                <div id="message">
                                    <h3>Password must contain the following:</h3>
                                    <p id="letter" class="invalid">A <b>lowercase</b> letter</p>
                                    <p id="capital" class="invalid">A <b>capital (uppercase)</b> letter</p>
                                    <p id="number" class="invalid">A <b>number</b></p>
                                    <p id="length" class="invalid">Minimum <b>8 characters</b></p>
                                    <p id="special" class="invalid"> <b>Special</b> characters</p>
                                </div>


                                <div class="form-group">
                                    <input type="submit" id="enablebtn" class="btn btn-primary btn-block btn-lg"
                                        value="Signup">
                                </div>
                            </div>
                    </form>

                </div>
                <div class="hint-text small"><a href="#">Forgot Your Password?</a></div>
